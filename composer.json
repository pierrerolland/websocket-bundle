{
  "name": "rollandrock/websocket-bundle",
  "type": "symfony-bundle",
  "description": "Ratchet abstraction for Symfony 4",
  "keywords": ["websocket"],
  "license": "MIT",
  "authors": [
    {
      "name": "Pierre Rolland",
      "email": "roll.pierre@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "symfony/framework-bundle": "~4.0"
  },
  "autoload": {
    "psr-4": { "RollandRock\\WebsocketBundle\\": "" },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": { "RollandRock\\WebsocketBundle\\Tests\\": "Tests" }
  }
}
